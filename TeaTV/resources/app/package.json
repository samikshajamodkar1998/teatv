    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-persist": "^5.2.2",
    "redux-thunk": "^2.2.0",
    "remote-file-size": "^3.0.4",
    "request": "^2.83.0",
    "sanitize-filename": "^1.6.1",
    "toastr": "^2.1.2",
    "trakt.tv": "^6.0.2",
    "unzip": "^0.1.11",
    "utf-8-validate": "^4.0.0",
    "utf8": "^2.1.2",
    "youtube-embed-video": "^1.0.2"
  }
