{
  "name": "TeaTV",
  "version": "1.5.0",
  "description": "TeaTV App - Free HD Movies ",
  "main": "app/main.js",
  "scripts": {
    "start": "electron .",
    "rebuild-node": "./node_modules/.bin/electron-rebuild",
    "build-win64": "electron-packager . TeaTV --platform win32 --arch x64 --overwrite --icon build/icon/icon.ico --out build/outputs/",
    "build-win32": "electron-packager . TeaTV --platform win32 --arch ia32 --overwrite --icon build/icon/icon.ico --out build/outputs/",
    "copy-win64": "robocopy node_modules/@appota TeaTV-win32-x64/resources/app/node_modules/@appota /e",
    "copy-win32": "robocopy node_modules/@appota TeaTV-win32-ia32/resources/app/node_modules/@appota /e",
    "webpack": "webpack --progress --colors --watch",
    "webpack-build": "webpack -p --progress --colors --watch",
    "list-codesign": "security find-identity -v -p codesigning",
    "update-source": "yarn upgrade @appota/provider-crawl @appota/url-resolver"
  },
  "author": "Le Quang Huy",
  "license": "CC0-1.0",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-async-to-module-method": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.7",
    "electron": "~9.4.0",
    "electron-osx-sign": "^0.4.7",
    "electron-packager": "^10.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "@appota/electron-renderer-updater": "git+https://git.appota.com/huylq/electron-renderer-updater.git",
    "@appota/error-logger": "git+https://git.appota.com/huylq/error-logger.git",
    "@appota/provider-crawl": "git+https://git.appota.com/dongnt/provider-crawler.git",
    "@appota/url-resolver": "git+https://git.appota.com/huylq/url-resolver.git",
    "@exponent/electron-cookies": "^2.0.0",
    "axios": "^0.16.2",
    "babel-polyfill": "^6.26.0",
    "castv2-client": "^1.2.0",
    "compare-versions": "^3.1.0",
    "cross-fetch": "^2.1.0",
    "dom-storage": "^2.0.2",
    "electron-ga": "^1.0.5",
    "electron-google-analytics": "^0.0.17",
    "electron-is-dev": "^0.3.0",
    "electron-json-storage": "^3.2.0",
    "electron-rebuild": "^1.6.0",
    "ffbinaries": "^1.0.3",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^4.0.2",
    "history": "^4.7.2",
    "iconv-lite": "^0.4.19",
    "jquery": "^3.2.1",
    "js-events-listener": "^1.0.0",
    "md5": "^2.2.1",
